package oops;

class A
{
	int i = 10;
	
	public void display()
	{
		System.out.println(i);
		System.out.println("running class a display");
	}
}

class B extends A
{
	int i=50;
	public void show()
	{
		System.out.println(i);
		System.out.println("running show");
	}
}

public class O006_InheritanceDemo {
	public static void main(String[] args) {
		
		B b = new B();
		b.show();
		
	}
}
