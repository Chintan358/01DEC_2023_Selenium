package oops;

class Pen
{
	int price = 10;
	String color="red";
	String company="cello";
	
	public void toWrite()
	{
		System.out.println(price+" "+color+" "+company);
	}
}

public class O001_ClassDemo {
	public static void main(String[] args) {
		
		Pen p = new Pen();
		p.price=70;
		p.toWrite();
		
		Pen p1 = new Pen();
		p1.toWrite();
		
	}
}
