package oops;

class Calc
{
	public void display()
	{
		System.out.println("Runing display");
	}
	
	public void square(int a)
	{
		int sq = a*a;
		System.out.println("sq of"+a+" is "+sq);
		
	}
}

public class O003_MetodManipulation {
	public static void main(String[] args) {
		
		Calc c = new Calc();
		c.display();
		c.square(10);
		
		
		
	}
}
