package oops;

class Student
{
	int id;
	String name;
	
	Student()
	{
		System.out.println("constructor calling...");
	}
	
	public void display()
	{
		System.out.println(id+" "+name);
	}
}

public class O002_constructorDemo {
	public static void main(String[] args) {
		
		
		Student st = new Student();
		st.display();
		
		
	}
}
