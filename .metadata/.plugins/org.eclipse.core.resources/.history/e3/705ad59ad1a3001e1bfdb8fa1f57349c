package basicpractical;

public class Armstrong {
	public static void main(String[] args) {
		
		
		int num = 153;
		int sum = 0;
		
		
		while(num!=0)
		{
			int rem = num%10;            // 3 5 1
			System.out.println(rem);
			num = num/10;               //15 1
			System.out.println(num);
		}
	}
}
